    "watch": "clear; echo @dogu-tech/node Watching... && nodemon"
  },
  "types": "build/src/index.d.ts",
  "devDependencies": {
    "@dogu-dev-private/publish-package": "workspace:^",
    "@types/node": "18.15.11",
    "cross-env": "7.0.3",
    "nodemon": "2.0.22",
    "shx": "0.3.4",
    "ts-node": "10.9.1",
    "typescript": "4.7.4"
  },
  "publishConfig": {
    "access": "public"
  }
}
