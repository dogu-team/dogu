    "fast-glob": "3.3.0",
    "jest": "29.6.0",
    "nodemon": "2.0.22",
    "shx": "0.3.4",
    "ts-jest": "29.1.1",
    "ts-node": "10.9.1",
    "typescript": "4.7.4"
