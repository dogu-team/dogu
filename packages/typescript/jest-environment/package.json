  "type": "module",
  "exports": {
    ".": {
      "require": {
        "types": "./build/cjs/src/environment/index.d.ts",
        "default": "./build/cjs/src/environment/index.js"
      },
      "import": {
        "types": "./build/esm/src/environment/index.d.ts",
