{
  "name": "@dogu-tech/sdk-core",
  "version": "0.0.3",
  "license": "MIT License",
  "author": "Dogu Technologies",
  "type": "module",
  "exports": {
    ".": {
      "require": {
        "types": "./build/cjs/src/index.d.ts",
        "default": "./build/cjs/src/index.js"
      },
      "import": {
        "types": "./build/esm/src/index.d.ts",
        "default": "./build/esm/src/index.js"
      }
    }
  },
  "main": "build/cjs/src/index.js",
  "bin": {
    "dogu-sdk-core": "./build/esm/src/cli.js"
  },
  "files": [
    "build"
  ],
  "scripts": {
    "build": "node scripts/build.mjs",
    "clean": "shx rm -rf build build-info",
    "dev": "nodemon",
    "rebuild": "yarn run clean && yarn run build",
    "test": "cross-env NODE_OPTIONS=\"--experimental-vm-modules $NODE_OPTIONS\" jest",
    "watch": "clear; echo @dogu-tech/sdk-core Watching... && nodemon"
  },
  "types": "build/cjs/src/index.d.ts",
  "dependencies": {
    "@nestjs/common": "10.1.3",
    "@nestjs/core": "10.1.3",
    "@nestjs/platform-express": "10.1.3",
    "@nestjs/platform-ws": "10.1.3",
    "@nestjs/websockets": "10.1.3",
    "jsonschema": "1.4.1",
    "lodash": "4.17.21"
  },
  "devDependencies": {
    "@babel/cli": "7.22.9",
    "@babel/core": "7.22.9",
    "@babel/plugin-transform-modules-commonjs": "7.22.5",
    "@babel/preset-typescript": "7.22.5",
    "@jest/globals": "29.6.0",
    "@jest/types": "29.6.0",
    "@types/babel__core": "7",
    "@types/babel__preset-env": "7",
    "@types/lodash": "4",
    "@types/node": "18.15.11",
    "babel-plugin-transform-import-meta": "2.2.0",
    "cross-env": "7.0.3",
    "fast-glob": "3.3.1",
    "jest": "29.6.0",
    "nodemon": "2.0.22",
    "shx": "0.3.4",
    "ts-jest": "29.1.1",
    "ts-node": "10.9.1",
    "typescript": "4.7.4"
  },
  "publishConfig": {
    "access": "public"
  },
  "nodemonConfig": {
    "verbose": true,
    "watch": [
      "./src"
    ],
    "exec": "yarn run build",
    "ext": "ts"
  },
  "url": "https://github.com/dogu-team/dogu"
}
