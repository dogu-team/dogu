{
  "private": true,
  "name": "dogu-agent",
  "version": "2.9.0",
  "description": "Dogu Agent",
  "license": "SEE LICENSE IN https://github.com/dogu-team/dogu/blob/main/LICENSE.md",
  "author": "Dogu Technologies",
  "homepage": ".",
  "main": "build/electron/main.js",
  "scripts": {
    "build": "yarn run build:react && yarn run build:electron",
    "build:electron": "tsc -p electron",
    "build:react": "cross-env CI=true react-app-rewired build",
    "clean": "shx rm -rf prepare build dist generated/logs",
    "deploy:electron": "yarn run build && ts-node scripts/build",
    "deploy:notarize": "yarn run build && ts-node scripts/build --notarize=true",
    "deploy:publish": "yarn run build && ts-node scripts/build --notarize=true --publish=true",
    "dev": "nodemon",
    "eject": "react-app-rewired eject",
    "rebuild": "yarn run clean && yarn run build",
    "start": "shx rm -rf generated/logs && concurrently \"yarn build:electron && wait-on http://127.0.0.1:3333 && electron .\" \"cross-env BROWSER=none yarn start:react\" ",
    "start:react": "cross-env PORT=3333 react-app-rewired start",
    "test:dev": "react-app-rewired test",
    "util:set-productname": "ts-node scripts/util/set-productname.ts name",
    "util:set-version": "ts-node scripts/util/set-version.ts version"
  },
  "dependencies": {
    "@chakra-ui/checkbox": "2.2.13",
    "@chakra-ui/icons": "2.0.17",
    "@chakra-ui/react": "2.5.2",
    "@chakra-ui/system": "2.5.3",
    "@dogu-private/device-server": "workspace:^",
    "@dogu-private/dogu-agent-core": "workspace:^",
    "@dogu-private/dost-children": "workspace:^",
    "@dogu-private/host-agent": "workspace:^",
    "@dogu-private/types": "workspace:^",
    "@dogu-tech/common": "workspace:^",
    "@dogu-tech/device-client": "workspace:^",
    "@dogu-tech/device-client-common": "workspace:^",
    "@emotion/react": "11.10.6",
    "@emotion/styled": "11.10.6",
    "@sentry/electron": "4.5.0",
    "@sentry/react": "7.50.0",
    "ansi-to-html": "0.7.2",
    "compressing": "1.9.0",
    "dotenv": "16.0.3",
    "electron-devtools-installer": "3.2.0",
    "electron-is-dev": "2.0.0",
    "electron-log": "4.4.8",
    "electron-store": "8.1.0",
    "electron-window-state": "^5.0.3",
    "find-free-ports": "3.0.0",
    "framer-motion": "10.6.1",
    "lodash": "4.17.21",
    "pidtree": "0.6.0",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "react-icons": "4.8.0",
    "react-router-dom": "6.11.2",
    "shelljs": "0.8.5",
    "styled-components": "6.0.0-rc.2",
    "styled-reset": "4.4.7",
    "systeminformation": "5.17.14",
    "web-vitals": "3.1.1",
    "ws": "8.13.0",
    "zustand": "4.3.8"
  },
  "devDependencies": {
    "@testing-library/dom": "9.0.1",
