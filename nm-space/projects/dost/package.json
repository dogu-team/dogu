  "main": "build/electron/main.js",
  "scripts": {
    "build": "yarn run build:react && yarn run build:electron",
    "build:electron": "tsc -p electron",
    "build:react": "cross-env CI=true react-app-rewired build",
    "clean": "shx rm -rf prepare build dist generated",
    "debug": "yarn run build:react && nodemon --debug 60013",
    "deploy": "tsc -p electron && ts-node scripts/build --upload=true",
    "deploy:electron": "tsc -p electron && ts-node scripts/build",
    "deploy:publish": "tsc -p electron && ts-node scripts/build --notarize=true --upload=true --publish=true",
    "deploy:notarize:upload": "tsc -p electron && ts-node scripts/build --notarize=true --upload=true",
    "dev": "yarn run build:react && nodemon",
    "eject": "react-app-rewired eject",
    "rebuild": "yarn run clean && yarn run build",
    "start": "cross-env PORT=3333 react-app-rewired start",
    "test": "react-app-rewired test",
    "util:set-productname": "ts-node scripts/util/set-productname.ts name",
    "util:set-version": "ts-node scripts/util/set-version.ts version"
  },
