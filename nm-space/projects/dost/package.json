    "@testing-library/user-event": "14.4.3",
    "@types/electron-devtools-installer": "2.2.2",
    "@types/eslint": "8",
    "@types/jest": "29.4.0",
    "@types/lodash": "4.14.186",
    "@types/node": "18.15.11",
    "@types/react": "18.0.28",
    "@types/react-dom": "18.0.11",
    "@types/shelljs": "0.8.11",
    "@types/styled-components": "5.1.26",
    "@types/ws": "8",
