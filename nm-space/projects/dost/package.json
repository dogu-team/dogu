    "@typescript-eslint/parser": "6.9.0",
    "class-transformer": "0.5.1",
    "class-validator": "0.14.0",
    "concurrently": "7.6.0",
    "cross-env": "7.0.3",
    "electron": "23.1.0",
    "electron-builder": "23.6.0",
    "eslint": "8.52.0",
    "extract-zip": "2.0.1",
    "node-fetch": "2.6.7",
    "nodemon": "2.0.22",
    "react-app-rewired": "2.2.1",
    "react-scripts": "5.0.1",
    "shx": "0.3.4",
    "ts-node": "10.9.1",
    "typescript": "4.7.4",
    "wait-on": "7.0.1",
    "yargs": "17.7.1"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ]
  },
  "packageManager": "yarn@3.3.1",
  "productName": "Dogu-Agent",
  "url": "https://github.com/dogu-team/dogu"
}
