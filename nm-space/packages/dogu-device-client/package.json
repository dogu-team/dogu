{
  "name": "dogu-device-client",
  "version": "0.0.3",
  "license": "MIT License",
  "author": "Dogu Technologies",
  "type": "module",
  "exports": {
    ".": {
      "import": {
        "types": "./build/types/src/index.d.ts",
        "default": "./build/esm/src/index.js"
      },
      "require": {
        "types": "./build/types/src/index.d.ts",
        "default": "./build/cjs/src/index.js"
      }
    }
  },
  "main": "build/cjs/src/index.js",
  "files": [
    "build"
  ],
  "scripts": {
    "build": "ts-node --esm --project configs/tsconfig.dev.json scripts/build.ts",
    "clean": "shx rm -rf build build-temp",
    "dev": "nodemon",
    "rebuild": "yarn run clean && yarn run build",
    "start": "node build/esm/src/cli.js",
    "test": "cross-env NODE_OPTIONS=\"--experimental-vm-modules $NODE_OPTIONS\" jest --runInBand",
    "watch": "clear; echo dogu-device-client Watching... && nodemon"
  },
  "types": "build/types/src/index.d.ts",
  "dependencies": {
    "axios": "1.3.5",
    "class-transformer": "0.5.1",
    "class-validator": "0.14.0",
    "lodash": "4.17.21",
    "reflect-metadata": "0.1.13"
  },
  "devDependencies": {
    "@babel/cli": "7.22.9",
    "@babel/core": "7.22.9",
    "@babel/plugin-proposal-decorators": "7.22.10",
    "@babel/plugin-transform-class-properties": "7.22.5",
    "@babel/plugin-transform-modules-commonjs": "7.22.5",
    "@babel/preset-typescript": "7.22.5",
    "@jest/globals": "29.6.0",
    "@jest/types": "29.6.0",
    "@types/babel__core": "7",
    "@types/babel__preset-env": "7",
    "@types/lodash": "4",
    "@types/node": "18.15.11",
    "babel-plugin-transform-commonjs": "1.1.6",
    "babel-plugin-transform-import-meta": "2.2.0",
    "cross-env": "7.0.3",
    "jest": "29.6.0",
    "nodemon": "2.0.22",
    "shx": "0.3.4",
    "ts-jest": "29.1.1",
    "ts-node": "10.9.1",
    "typescript": "4.7.4"
  },
  "publishConfig": {
    "access": "public"
  },
  "nodemonConfig": {
    "verbose": true,
    "watch": [
      "./src"
    ],
    "exec": "yarn ts-node --project ./configs/tsconfig.dev.json --esm src/cli.ts",
    "ext": "ts"
  },
  "url": "https://github.com/dogu-team/dogu"
}
